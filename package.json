    "eslint": "^9.9.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-promise": "^7.1.0",
    "husky": "^9.1.6",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2"
